{
  "name": "flashtest",
  "version": "0.0.4",
  "main": "build/index.js",
  "license": "MIT",
  "author": "Yurii Fediv (alpachino48@gmail.com)",
  "description": "Generate tests written with jest and puppeteer from interacting with UI.",
  "keywords": [
    "jest",
    "puppeteer",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/bondom/flashtest.git"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev-api\" \"npm run dev-sandbox\"",
    "dev-sandbox": "webpack-dev-server --mode development --config ./TestSandbox/webpack.config.js",
    "dev-api": "ts-node-dev --project ./tsconfig.json TestSandbox/testApi --ignore-watch src",
    "server": "ts-node --project ./tsconfig.json --ignore-watch TestSandbox src/server/",
    "test": "npm run test:browser-u && npm run test:enzyme",
    "test:enzyme": "jest --config ./testConfig/enzyme/config.json",
    "test:enzyme-watch": "cross-env OFF_CONSOLE=true jest --config ./testConfig/enzyme/config.json --watch",
    "test:browser": "jest --config ./testConfig/puppeteer/config.json",
    "test:browser-u": "cross-env JEST_UPDATE_DATA_ENV=true jest --config ./testConfig/puppeteer/config.json",
    "test:browser-live": "cross-env JEST_PUPPETEER_CONFIG=./testConfig/puppeteer/jest-puppeteer.config.js jest --config ./testConfig/puppeteer/config.json",
    "prepush": "npm test",
    "fix": "eslint --ext .ts,.tsx --fix src/",
    "fix-sb": "eslint --ext .ts,.tsx --fix TestSandbox/",
    "build-client": "cross-env OFF_CONSOLE=true webpack --mode=development --config src/webpack.config.js",
    "build-server": "cross-env OFF_CONSOLE=true tsc -p tsconfig.prod.json",
    "build": "npm run build-client && npm run build-server",
    "prepublishOnly": "npm run build"
  },
  "bin": {
    "flashtest-server": "./build/server/index.js"
  },
  "dependencies": {
    "app-root-path": "^2.1.0",
    "glob-promise": "^3.4.0",
    "node-fs-extra": "^0.8.2",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-router-dom": "^4.1.2",
    "react-validation-utils": "^1.3.5",
    "prettier-eslint": "^8.8.1"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/cors": "^2.8.4",
    "@types/enzyme": "^3.1.9",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/express": "^4.11.1",
    "@types/fetch-mock": "^6.0.1",
    "@types/history": "^4.6.2",
    "@types/jest": "^22.2.3",
    "@types/node": "^9.6.18",
    "@types/puppeteer": "^1.6.0",
    "@types/react": "^16.3.9",
    "@types/react-dom": "^16.0.5",
    "@types/react-router-dom": "^4.2.6",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "concurrently": "^3.6.1",
    "cors": "^2.8.4",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "enzyme-to-json": "^3.2.2",
    "eslint": "4.12.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-prettier": "2.3.1",
    "eslint-plugin-react": "^7.1.0",
    "express": "^4.16.3",
    "fetch-mock": "^6.3.0",
    "file-loader": "^1.1.11",
    "fork-ts-checker-webpack-plugin": "^0.4.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.0.6",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.4",
    "jest-css-modules": "^1.1.0",
    "jest-environment-node": "^23.1.0",
    "jest-puppeteer": "^3.0.1",
    "mini-css-extract-plugin": "^0.2.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "prettier": "^1.9.0",
    "prop-types": "^15.6.2",
    "puppeteer": "^1.6.1",
    "react-hot-loader": "next",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "scss-lint": "^0.0.0",
    "style-loader": "^0.17.0",
    "ts-jest": "^22.4.6",
    "ts-loader": "^4.1.0",
    "ts-node": "^7.0.1",
    "ts-node-dev": "^1.0.0-pre.25",
    "typescript": "2.9.1",
    "typescript-eslint-parser": "^16.0.0",
    "typings-for-css-modules-loader": "^1.7.0",
    "uglifyjs-webpack-plugin": "^1.2.3",
    "url-loader": "^0.5.9",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.1"
  }
}
